{
  "name": "fixrbot",
  "version": "0.0.1",
  "description": "A probot App that analyses the API usage patterns.",
  "author": "Lesley Lai <lesleylai@lesleylai.info> (https://github.com/cuplv/Fixrbot)",
  "license": "ISC",
  "repository": "https://github.com//Fixrbot.git",
  "homepage": "https://github.com//Fixrbot",
  "bugs": "https://github.com//Fixrbot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postinstall": "npm run build",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./build/index.js",
    "lint": "tslint --project .",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "prettier": "prettier --write {src,test}/**/*.ts"
  },
  "dependencies": {
    "@types/nock": "^9.3.0",
    "@types/node-fetch": "^2.3.5",
    "es6-promise": "^4.2.6",
    "isomorphic-fetch": "^2.2.1",
    "nock": "^10.0.0",
    "node-fetch": "^2.6.1",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.13",
    "husky": "^2.4.0",
    "jest": "^24.0.0",
    "lint-staged": "^8.2.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "smee-client": "^1.0.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
